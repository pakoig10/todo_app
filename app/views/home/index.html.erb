<%- if user_signed_in?%>
    <h1>Home#index</h1>
    <p>Hello <%= current_user.first_name %> <%= current_user.last_name %> </p>
    <%= link_to 'Log out', destroy_user_session_path, method: :delete  %>
    <%= link_to 'Edit your profile', edit_user_registration_path %>
    <%= link_to 'Lists', lists_path %>
<% else %>
    <%= link_to 'login', new_user_session_path %>
    <%= link_to 'sign in', new_user_registration_path %>
<%end%>